                    <div class="ibox float-e-margins">
                        <div class="ibox-content">
                            <div class="file-manager">
                                <h5>Folders</h5>
                                <ul class="folder-list" style="padding: 0">
                                    <% fileInfo.forEach(function (item) { %>
                                    <li><a href=""><i class="fa fa-folder"></i> <%= item %></a></li>
                                    <% }) %>
                                </ul>
                                <div class="clearfix"></div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-lg-9 animated fadeInRight">
                    <div class="row">
                        <div class="col-lg-12">
                            <iframe id="file" name="file" src="<%= settings.stServer %>/uploads" frameborder="0"   style="border:none;overflow-y:auto;height:500px;width:100%;backgrounnd-color:transparent"></iframe>
                        </div>
                    </div>
                </div>
